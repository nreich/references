<!DOCTYPE html>
<html>
<head>
  <title><%= title %></title>
  <%= csrf_meta_tags %>
  <%= javascript_include_tag 'jquery-1.7.2.min.js', 'jquery-ui-1.8.22.custom.min.js' %>
  <%= stylesheet_link_tag 'jquery-ui-1.8.22.custom.css'%>
  <%= render 'layouts/stylesheets' %>
</head>
<body>
  <div class="container">
    <%= render 'layouts/header' %>
    <selection class="round">
    <% flash.each do |key, value| %>
      <div class="flash <%= key %>"><%= value %></div>
    <% end %>
    <%= yield %>
    </selection>
    <%= debug(params) if Rails.env.development? %>
  </div>
</body>
</html>
